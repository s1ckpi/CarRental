<Page x:Class="CarRental.Views.AutomobilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CarRental.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AutomobilePage">

    <Grid Background="#cccdd1">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="300"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>

        <Frame x:Name="ListAutomobileFrame" Grid.Row="0" Grid.Column="0" ContentRendered="ListAutomobileFrameContentRendered" NavigationUIVisibility="Hidden"/>
        <Frame x:Name="AutomobileFrame" Grid.Row="0" Grid.Column="1" ContentRendered="AutomobileFrameContentRendered" NavigationUIVisibility="Hidden"/>
        <Frame x:Name="MiniProfileFrame" Grid.Row="1" Grid.Column="0" ContentRendered="MiniProfileFrameContentRendered" NavigationUIVisibility="Hidden"/>
    </Grid>
</Page>
